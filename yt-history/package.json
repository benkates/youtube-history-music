{
  "name": "yt-history",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@observablehq/plot": "^0.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "d3": "^7.6.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp -r build/* ~/Documents/personal_site/apps/yt-history && rm -rf build",
    "git-add": "git -C ~/Documents/personal_site/ add .",
    "git-commit": "git -C ~/Documents/personal_site/ commit -m ",
    "git-push": "git -C ~/Documents/personal_site/ push origin",
    "git-bacp": "echo 'Enter Commit Message' && read message && npm run build && npm run git-add && npm run git-commit \"$message\" && npm run git-push"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
